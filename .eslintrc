{
  "extends": ["airbnb-base", "eslint:recommended"],
  "env": {
    "browser":true
  },
  "rules": {
    "no-underscore-dangle": [
      "error",
      {
        "allow": [
          "_id"
        ]
      }
    ],
    "quotes": [0, "single", "double"],
    "func-names": ["error", "never"],
    "no-console": "off"
  }
}